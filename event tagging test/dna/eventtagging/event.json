{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "id": "http://example.com/eventtagging/event.json",
  "type": "object",
  "required": [
    "name",
    "tags"
  ],
  "properties": {
    "date": {
      "type": "string",
      "pattern": ".+"
    },
    "name": {
      "type": "string",
      "pattern": ".+"
    },
    "tags": {
      "default": [],
      "items": {
        "type": "string",
        "pattern": ".+"
      },
      "type": "array"
    }
  }
}